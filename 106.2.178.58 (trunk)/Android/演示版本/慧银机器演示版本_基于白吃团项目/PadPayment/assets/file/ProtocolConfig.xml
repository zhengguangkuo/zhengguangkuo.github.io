<?xml version="1.0" encoding="UTF-8"?>
<PROTOCOLDEF>
    <PROTOCOL>
        <ID>TCP_SHORT_1</ID><!-- 协议名称 -->
        <TYPE>TCP</TYPE><!-- 协议的类型 -->
        <ASYNMODE>syn</ASYNMODE>
        <MODE>0</MODE><!-- 长连接还是短连接  0短连接 1长连接-->
        <HOST>192.168.16.113</HOST><!-- 协议的IP -->
        <PORT>59999</PORT><!-- 协议的端口 -->
        <ENCODING>UTF-8</ENCODING><!-- 通讯所使用的编码 -->
        <POLICY>length:unknown</POLICY><!-- 读写策略 -->
        <READTIMEOUT>0</READTIMEOUT><!-- 读超时时间 -->
        <SERVERSIDE>false</SERVERSIDE><!-- 是否是server端 -->
        <COOLPOOLSIZE>10</COOLPOOLSIZE><!-- 空闲线程数 -->
        <MAXINUMPOOLSIZE>20</MAXINUMPOOLSIZE><!-- 最大线程数量 -->
    </PROTOCOL>
</PROTOCOLDEF>
